{
  "name": "assurance-ai-agent",
  "version": "1.0.0",
  "type": "module",
  "description": "AI agent for Adobe Assurance debugging",
  "main": "server.js",
  "scripts": {
    "start": "node server.js",
    "dev": "node --watch server.js",
    "test": "node tests/test-ollama.js",
    "test:upload": "node tests/test-chunked-upload.js",
    "test:rag": "node tests/test-rag.js",
    "test:kb-dedup": "node tests/test-kb-deduplication.js",
    "test:all": "node tests/test-ollama.js && node tests/test-rag.js",
    "kb:populate": "node scripts/populate-knowledge-base.js",
    "kb:load": "node scripts/load-all-batches.js",
    "kb:load-manual": "node scripts/load-batch-manual.js",
    "kb:clear": "node scripts/clear-knowledge-base.js",
    "kb:reset": "node scripts/clear-knowledge-base.js && node scripts/populate-knowledge-base.js",
    "kb:export": "curl -s http://localhost:3001/api/knowledge/documents | node -e \"const data=JSON.parse(require('fs').readFileSync(0,'utf-8')); const urls=data.documents.map(d=>d.url); console.log(JSON.stringify({metadata:{total:data.total,date:new Date().toISOString()},urls},null,2))\" > docs/knowledge-base-urls.json && echo 'âœ… Exported URLs to docs/knowledge-base-urls.json'"
  },
  "keywords": [
    "adobe",
    "assurance",
    "ai",
    "debugging",
    "ollama",
    "langchain"
  ],
  "author": "sagsharma@adobe.com",
  "license": "Apache-2.0",
  "packageManager": "yarn@1.22.22+sha512.a6b2f7906b721bba3d67d4aff083df04dad64c399707841b7acf00f6b133b7ac24255f2652fa22ae3534329dc6180534e98d17432037ff6fd140556e2bb3137e",
  "dependencies": {
    "@langchain/community": "^0.3.58",
    "@langchain/core": "^1.1.4",
    "@langchain/langgraph": "^1.0.4",
    "@langchain/ollama": "^1.0.3",
    "@langchain/textsplitters": "^1.0.1",
    "axios": "^1.13.2",
    "cheerio": "^1.1.2",
    "chromadb": "^3.1.6",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.2.1",
    "hnswlib-node": "^3.0.0",
    "langchain": "^1.1.5",
    "multer": "^2.0.2",
    "pdf-parse": "^2.4.5",
    "uuid": "^13.0.0"
  }
}
